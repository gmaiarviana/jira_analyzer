{
  "name": "jira-analyzer",
  "version": "1.0.0",
  "description": "Sistema para extração de dados do JIRA via JQL e análise conversacional com GitHub Copilot",
  "main": "src/main.ts",
  "scripts": {
    "analyze": "tsx src/main.ts",
    "dev": "tsx --watch src/main.ts",
    "build": "tsc",
    "start": "node dist/main.js",
    "lint": "eslint src/**/*.ts",
    "test": "jest"
